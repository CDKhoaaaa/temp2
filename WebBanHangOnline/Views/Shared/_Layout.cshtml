@using WebBanHangOnline.Common
<!DOCTYPE html>
<html>
<style>
    .container-fluid.bg-secondary.text-dark {
        padding: 60px 0; /* Adjust the padding as needed */
    }

    /* Style for the EShopper heading */
    .display-5.font-weight-semi-bold {
        color: #fff; /* Set the text color */
    }

    /* Style for the paragraphs */
    .container-fluid.bg-secondary.text-dark p {
        color: #ccc; /* Set the text color */
    }

        /* Style for the contact information */
        .container-fluid.bg-secondary.text-dark p i {
            color: #3498db; /* Set the icon color */
            margin-right: 50px; /* Adjust the spacing between icon and text */
        }

    /* Style for the Quick Links section */
    .font-weight-bold.text-dark.mb-4 {
        color: #fff; /* Set the text color */
    }

    /* Style for the links in the Quick Links section */
    .d-flex.flex-column.justify-content-start a.text-dark {
        color: #ccc; /* Set the link text color */
        text-decoration: none; /* Remove underline */
        transition: color 0.3s ease; /* Add a smooth color transition */
    }

        .d-flex.flex-column.justify-content-start a.text-dark:hover {
            color: #3498db; /* Change the link color on hover */
        }

    /* Style for the Subscribe section */
    #mc_embed_signup {
        padding: 10px; /* Adjust the padding as needed */
        border-radius: 4px;
    }

    #mc_embed_signup h2 {
        color: #333; /* Set the heading color */
    }
    #mc_embed_signup label {
        font-size: 12px; /* Adjust the font size of labels */
    }

        #mc_embed_signup input[type="email"],
        #mc_embed_signup input[type="text"] {
            width: 100%;
            height: 70%;
            border: 1px solid #ccc;
            border-radius: 4px;
            font-size: 10px;
        }

        #mc_embed_signup input[type="submit"] {
            background-color: #3498db;
            color: #fff;
            border: none;
            border-radius: 4px;
            cursor: pointer;
            transition: background-color 0.3s ease; 
        }

            #mc_embed_signup input[type="submit"]:hover {
                background-color: #267bbd; 
            }

    /* Style for the footer bottom section */
    .row.border-top.border-light.mx-xl-5.py-4 {
        background-color: #333;
    }

    .text-dark.font-weight-semi-bold {
        color: #fff; /* Set the text color */
    }

        .text-dark.font-weight-semi-bold:hover {
            color: #3498db; 
        }
</style>
